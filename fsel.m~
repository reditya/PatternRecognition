% feature selection brute force on dataset_features
train = trn_f;
test = tst% no 1-24 features
n = [1:1:24];
v = nchoosek(n,5); %choose best 5
k = [zeros(size(v,1),1) v];

min = 1;
for i = 1:length(v)
   feat = v(i,:);
   sTrain = train(:,feat);
   sTest = test(:,feat);
   W = fisherc(sTrain);
   [E,C] = testc(sTest*W);
   k(i,1) = E;
   if E < min
       optimal = feat;
       min = E;
   end
end
    
%[dWf,dRf] = featself(diabetes,'maha-s',5);
