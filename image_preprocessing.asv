close all;
clear;
clc;

prwaitbar off
prwarning off

nistdataset = prnist([0:9], [1:100:1000]);
img = nistdataset(10,:);
show(img);
figure;
preproc = im_rotate()*im_box([],0,1)*im_resize([], [16 16]);


preproc_img = img*preproc;
show(preproc_img);
%figure;
%show(reshape(+preproc_img,[16,16]));
figure;
show(nistdataset, 10);
figure;
show(nistdataset*preproc, 10)